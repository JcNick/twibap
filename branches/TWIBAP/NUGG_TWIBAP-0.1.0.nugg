 <PROG NAME="Z_TWA_DEMO" VARCL="X" SUBC="1" CNAM="SE38_UWE" CDAT="20100808" UNAM="SE38_UWE" UDAT="20100820" VERN="000012" RMAND="001" RLOAD="E" FIXPT="X" SDATE="20100820" STIME="132702" IDATE="20100820" ITIME="132702" UCCHECK="X">
